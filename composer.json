{
    "name": "mediamotionag/contao-simple-svg-icons-bundle",
    "type": "contao-bundle",
    "description": "Simply use SVG icons from an SVG icon sprite file with Contao inserttags.",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "slashworks extended by mediamotion",
            "homepage": "https://github.com/slashworks"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "contao/core-bundle": "^4.4"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Slashworks\\ContaoSimpleSvgIconsBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Slashworks\\ContaoSimpleSvgIconsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slashworks\\ContaoSimpleSvgIconsBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/slashworks/contao-simple-svg-icons-bundle/issues",
        "source": "https://github.com/slashworks/contao-simple-svg-icons-bundle"
    }
}
